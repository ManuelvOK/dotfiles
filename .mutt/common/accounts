################################################
### General Account Settings
################################################

# Header Cache
set header_cache="$my_account_dir/headercache"

# Mailboxes
source $my_account_dir/mailboxes

# Keybindings
bind index,pager        g       noop
macro index,pager       gg      "<change-folder>=$my_account/INBOX<enter>"      "go to INBOX"

